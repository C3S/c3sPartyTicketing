<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="c3spartyticketing">
  <head>
    <title i18n:translate="">C3S Event Ticketing</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8"/>
    <meta name="keywords" content="Ticketing for C3S SCE"/>
    <meta name="description" content="" />
    <link rel="shortcut icon" href="${request.static_url('c3spartyticketing:static/favicon.ico')}" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <link href="${request.static_url('c3spartyticketing:static/bootstrap-3.0.3-dist/css/bootstrap.css')}" rel="stylesheet"/>
    <!-- JavaScript -->
    <script src="${request.static_url('c3spartyticketing:static/jquery/jquery-2.1.1.min.js')}"
            type="text/javascript"></script>
    <tal:block metal:define-slot="deform-js-css"></tal:block>
    <tal:block metal:define-slot="meta"></tal:block>
  </head>
  <body>
    <tal:block metal:define-slot="top">
      <a href="/">
        <img src="${request.static_url('c3spartyticketing:static/images/c3s_logo_horiz_500x100.png')}"
             class="img-responsive"
	     alt="Cultural Commons Collecting Society (C3S)"/>
      </a>
      <div style="text-align:right;">
        <a href="${request.current_route_url(_query='en')}">
          <img alt="switch to English language" title="English" src="/static/images/en.png" />
        </a>
        <a href="${request.current_route_url(_query='de')}">
          <img alt="auf deutsche Sprache umschalten" title="Deutsch" src="/static/images/de.png" />
        </a>
      </div>
    </tal:block>
    <tal:block metal:define-slot="middle"></tal:block>
    <tal:block metal:define-slot="bottom"></tal:block>
    <tal:block metal:define-slot="footer">
      <div class="footer">
        &copy; <span i18n:translate="">Copyright 2015, C3S SCE.</span> 
        <a i18n:attributes="href"
           i18n:name=""
           href="https://www.c3s.cc/en/contact-us/"
            i18n:translate="">Contact</a>
        &amp;
        <a i18n:attributes="href"
           i18n:name=""
           i18n:translate=""
           href="https://www.c3s.cc/en/impressum/">Imprint</a>
      </div>
    </tal:block>
  </body>
</html>
